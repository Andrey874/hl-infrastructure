{
	"service": {
	  "id": "{{ service_name }}",
	  "name": "{{ service_name }}",
	  "tags": [ "{{ service_name }}" ], 
	  "port": 80,
	  "enable_tag_override": false,
	  "check": {
            "id": "web_up",
            "name": "nginx healthcheck",
            "args": [ "curl", "localhost" ],
            "interval": "10s",
            "timeout": "2s"
	  }
	}
}
